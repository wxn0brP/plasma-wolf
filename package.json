{
    "name": "@wxn0brp/plasma-wolf",
    "version": "0.0.6",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "description": "A radial context menu library",
    "repository": {
        "type": "git",
        "url": "https://github.com/wxn0brP/plasma-wolf.git"
    },
    "homepage": "https://github.com/wxn0brP/plasma-wolf",
    "author": "wxn0brP",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "build": "tsc && tsc-alias",
        "build-public": "bun build public/index.ts --outdir public --sourcemap --minify"
    },
    "dependencies": {
        "@wxn0brp/event-emitter": "^0.0.4"
    },
    "devDependencies": {
        "@types/bun": "*",
        "@wxn0brp/flanker-dialog": "^0.0.5",
